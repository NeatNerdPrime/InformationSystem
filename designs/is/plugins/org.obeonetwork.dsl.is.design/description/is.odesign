        <toolSections name="Binding">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="New binding" forceRefresh="true" nodeMappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@nodeMappings[name='DBI_BindingInfo']">
            <variable name="container">
              <subVariables xsi:type="tool:SelectModelElementVariable" name="left" candidatesExpression="feature:eAllContents" tree="true" rootExpression="[oclAsType(soa::Category).ownedCategories->union(oclAsType(environment::TypesDefinition).types)/]" childrenExpression="feature:types" message="Choose the first bound element (must be a DTO)"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="bindableElements" computationExpression="[left.oclAsType(soa::ServiceDTO).getAllBindableElements()->asSet() - container.oclAsType(soa::Category).getBoundElementsWithTarget(left.oclAsType(ecore::EObject))->asSet()/]"/>
              <subVariables xsi:type="tool:SelectModelElementVariable" name="right" candidatesExpression="[bindableElements.oclAsType(OclAny)->asSequence()->union(bindableElements.ancestors()->asSequence())->asOrderedSet()/]" tree="true" rootExpression="service:allRoots()" childrenExpression="feature:eContents" message="Choose the second bound element (must be a DTO or Entity)"/>
            </variable>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[container.oclAsType(soa::Category).getGlobalBindingRegistry()/]">
                <subModelOperations xsi:type="tool:If" conditionExpression="[left.oclIsKindOf(environment::DTO) and (right.oclIsKindOf(environment::DTO) or right.oclIsKindOf(entity::Entity))/]">
                  <subModelOperations xsi:type="tool:CreateInstance" typeName="environment.BindingInfo" referenceName="bindingInfos">
                    <subModelOperations xsi:type="tool:SetValue" featureName="left" valueExpression="var:left"/>
                    <subModelOperations xsi:type="tool:SetValue" featureName="right" valueExpression="var:right"/>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool:If" conditionExpression="[not left.oclIsKindOf(environment::DTO) or (not right.oclIsKindOf(environment::DTO) and not right.oclIsKindOf(entity::Entity))/]">
                  <subModelOperations xsi:type="tool:ChangeContext" browseExpression="[displayWarningMessage('Binding information not created', 'Only DTO and Entities can be bound')/]"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Bind existing element" precondition="[not oclIsTypeOf(soa::Category)/]" forceRefresh="true" nodeMappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@nodeMappings[name='DBI_BindingInfo']" extraMappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@containerMappings[name='DBI_ServiceDTO_CM'] //@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@containerMappings[name='DBI_ExternalServiceDTO_CM'] //@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@containerMappings[name='DBI_Entity_CM']">
            <variable name="container">
              <subVariables xsi:type="tool:AcceleoVariable" name="category" computationExpression="[containerView.eContainer(viewpoint::DSemanticDiagram).target/]"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="bindableElements" computationExpression="[category.oclAsType(soa::Category).getAllBindableElementsFor(container.oclAsType(environment::ObeoDSMObject))->asSet() - category.oclAsType(soa::Category).getBoundElementsWithTarget(container.oclAsType(ecore::EObject))->asSet()/]"/>
              <subVariables xsi:type="tool:SelectModelElementVariable" name="secondElement" candidatesExpression="[bindableElements.oclAsType(OclAny)->asSequence()->union(bindableElements.ancestors()->asSequence())->asOrderedSet()/]" tree="true" rootExpression="service:allRoots" childrenExpression="feature:eContents" message="Choose the second bound element"/>
            </variable>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[container.oclAsType(soa::ServiceDTO).getGlobalBindingRegistry()/]">
                <subModelOperations xsi:type="tool:If" conditionExpression="[secondElement.oclIsKindOf(environment::DTO) or secondElement.oclIsKindOf(entity::Entity)/]">
                  <subModelOperations xsi:type="tool:CreateInstance" typeName="environment.BindingInfo" referenceName="bindingInfos">
                    <subModelOperations xsi:type="tool:SetValue" featureName="left" valueExpression="var:container"/>
                    <subModelOperations xsi:type="tool:SetValue" featureName="right" valueExpression="var:secondElement"/>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool:If" conditionExpression="[not secondElement.oclIsKindOf(environment::DTO) and not secondElement.oclIsKindOf(entity::Entity)/)">
                  <subModelOperations xsi:type="tool:ChangeContext" browseExpression="[displayWarningMessage('Binding information not created', 'Only DTO and Entities can be bound')/]"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:OperationAction" name="Change target" precondition="[views->size() = 1 and views.oclAsType(viewpoint::DDiagramElementContainer).actualMapping.name->select(s : String | s = 'DBI_BoundElement')->size() > 0/]" forceRefresh="true">
            <view name="views">
              <subVariables xsi:type="tool:AcceleoVariable" name="category" computationExpression="[views.eContainer(viewpoint::DSemanticDiagram).target/]"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="firstElement" computationExpression="[views.oclAsType(viewpoint::DEdge).sourceNode.oclAsType(viewpoint::DNode).target.oclAsType(environment::BindingInfo).getBindingOpposite(views.oclAsType(viewpoint::DEdge).targetNode.oclAsType(viewpoint::DNode).target)/]"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="bindableElements" computationExpression="[category.oclAsType(soa::Category).getAllBindableElementsFor(firstElement.oclAsType(environment::ObeoDSMObject))->asSet() - category.oclAsType(soa::Category).getBoundElementsWithTarget(firstElement)->asSet()/]"/>
              <subVariables xsi:type="tool:SelectModelElementVariable" name="secondElement" candidatesExpression="[bindableElements->including(bindableElements.ancestors())->asOrderedSet()/]" tree="true" rootExpression="[category.allRoots()/]" childrenExpression="feature:eContents" message="Choose the second bound element"/>
            </view>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[views.oclAsType(viewpoint::DEdge).sourceNode.oclAsType(viewpoint::DNode).target.oclAsType(environment::BindingInfo).changeBindingTarget(firstElement.oclAsType(environment::BoundableElement), secondElement.oclAsType(environment::BoundableElement))/]"/>
            </initialOperation>
          </ownedTools>
          <subSections name="Usability">
            <ownedTools xsi:type="tool:DeleteElementDescription" name="DEL_BoundElement_PreventDeletion">
              <element name="element"/>
              <elementView name="elementView"/>
              <containerView name="containerView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[displayWarningMessage('Could not delete', 'Only whole binding can be deleted')/]"/>
              </initialOperation>
            </ownedTools>
            <ownedTools xsi:type="tool:ReconnectEdgeDescription" name="REC_BoundElement" precondition="[diagram.target.oclAsType(soa::Category).checkIfReconnectBindingIsPossible(element.oclAsType(environment::BindingInfo), source.oclAsType(ecore::EObject), target.oclAsType(ecore::EObject))/]">
              <source name="source"/>
              <target name="target"/>
              <sourceView name="sourceView"/>
              <targetView name="targetView"/>
              <element name="element"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[element.oclAsType(environment::BindingInfo).reconnectBindingTarget(source.oclAsType(environment::BoundableElement), target.oclAsType(environment::BoundableElement))/]"/>
              </initialOperation>
              <edgeView name="edgeView"/>
            </ownedTools>
            <ownedTools xsi:type="description_2:DBindingEditorCreationDescription" name="CRE_BindingEditor" label="Binding Details" description="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='Binding%20Details']">
              <containerViewVariable name="containerView"/>
              <representationNameVariable name="name"/>
            </ownedTools>
            <ownedTools xsi:type="tool:DoubleClickDescription" name="DBL_OpenBindingEditor" mappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@nodeMappings[name='DBI_BindingInfo']">
              <element name="element"/>
              <elementView name="elementView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[element.oclAsType(environment::BindingInfo).openBindingEditor()/]"/>
              </initialOperation>
            </ownedTools>
            <ownedTools xsi:type="tool:DiagramCreationDescription" name="DTO Binding" diagramDescription="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']">
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:self"/>
              </initialOperation>
              <containerViewVariable name="containerView"/>
              <representationNameVariable name="diagramName"/>
            </ownedTools>
            <ownedTools xsi:type="tool:DeleteElementDescription" name="DEL_ServiceDTO_PreventDeletion">
              <element name="element"/>
              <elementView name="elementView"/>
              <containerView name="containerView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:self"/>
              </initialOperation>
            </ownedTools>
          </subSections>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
