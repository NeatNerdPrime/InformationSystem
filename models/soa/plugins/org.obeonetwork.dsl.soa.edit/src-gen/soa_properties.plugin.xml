<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.obeonetwork.org/dsl/soa/2.0.0"
            class="org.obeonetwork.dsl.soa.providers.SoaEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.obeonetwork.dsl.soa.providers.SoaPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

  <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.sirius.diagram"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.sirius.diagram">
         <propertyTab
               id="System"
               label="System"
               category="default">
         </propertyTab>

         <propertyTab
               id="Metadatas"
               label="Metadatas"
               category="extended"
               afterTab="System">
         </propertyTab>

         <propertyTab
               id="Component"
               label="Component"
               category="default">
         </propertyTab>

         <propertyTab
               id="Service"
               label="Service"
               category="default">
         </propertyTab>

         <propertyTab
               id="Wire"
               label="Wire"
               category="default">
         </propertyTab>

         <propertyTab
               id="Binding"
               label="Binding"
               category="default">
         </propertyTab>

         <propertyTab
               id="Interface"
               label="Interface"
               category="default">
         </propertyTab>

         <propertyTab
               id="Operation"
               label="Operation"
               category="default">
         </propertyTab>

         <propertyTab
               id="Parameter"
               label="Parameter"
               category="default">
         </propertyTab>

         <propertyTab
               id="DTORegistry"
               label="DTORegistry"
               category="default">
         </propertyTab>

         <propertyTab
               id="Category"
               label="Category"
               category="default">
         </propertyTab>

         <propertyTab
               id="ServiceDTO"
               label="ServiceDTO"
               category="default">
         </propertyTab>

      </propertyTabs>
   </extension>
